        <div class="alert alert-success">
            {{ message }}
        </div>
    {% endfor %}
    <h1>Ajouter une categorie</h1>

    {{ include('categorie/_form.html.twig') }}

    <a href="{{ path('categorie_index') }}" class="btn btn-info mx-3">Retour</a>
{% endblock %}
